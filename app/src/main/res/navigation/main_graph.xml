        android:id="@+id/addProjectFragment00"
        android:name="com.example.pvwatts.ui.home.addproject.AddProjectFragment00"
        android:label="fragment_add_project_00"
        tools:layout="@layout/fragment_add_project_00" >
        <action
            android:id="@+id/action_addProjectFragment00_to_addProjectFragment01"
